public class Numbers {

	private int no1;
	private int no2;
	private String operations;

	public int getNo1() {
		return no1;
	}

	public void setNo1(int no1) {
		this.no1 = no1;
	}

	public int getNo2() {
		return no2;
	}

	public void setNo2(int no2) {
		this.no2 = no2;
	}

	public String getOperations() {
		return operations;
	}

	public void setOperations(String operations) {
		this.operations = operations;
	}

	@Override
	public String toString() {
		return "Numbers [no1=" + no1 + ", no2=" + no2 + ", operations=" + operations
				+ "]";
	}

	public Numbers(int no1, int no2, String nextOperaion, String operations) {
		super();
		this.no1 = no1;
		this.no2 = no2;
		this.operations = operations;
	}

	public Numbers() {
		super();
		// TODO Auto-generated constructor stub
	}

}
